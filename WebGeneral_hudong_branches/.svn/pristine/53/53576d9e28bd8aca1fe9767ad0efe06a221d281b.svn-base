package com.honghe.tech.entity;

import java.util.Date;

/**
 * 评论表
 * @author  caoqian    20180124
 */
public class Comment {
    private Integer commentId;     //key
    private Integer commentUserId; //评论用户id
    private Integer activityId;    //活动id
    private Integer visible;       //评论是否可见，0：不可见，1：可见
    private Integer replayId;      //评论id，发布的评论默认为0
    private String content;        //评论内容
    private String spareA;         //备注字段
    private String spareB;         //备注字段
    private Date updateTime;       //评论时间

    public Comment() {
    }

    public Comment(Integer commentId, Integer commentUserId, Integer activityId, Integer visible, Integer replayId, String content, String spareA, String spareB, Date updateTime) {
        this.commentId = commentId;
        this.commentUserId = commentUserId;
        this.activityId = activityId;
        this.visible = visible;
        this.replayId = replayId;
        this.content = content;
        this.spareA = spareA;
        this.spareB = spareB;
        this.updateTime = updateTime;
    }

    public Comment(Integer commentId, Integer commentUserId) {
        this.commentId = commentId;
        this.commentUserId = commentUserId;
    }

    public Integer getCommentId() {
        return commentId;
    }

    public void setCommentId(Integer commentId) {
        this.commentId = commentId;
    }

    public Integer getCommentUserId() {
        return commentUserId;
    }

    public void setCommentUserId(Integer commentUserId) {
        this.commentUserId = commentUserId;
    }

    public Integer getActivityId() {
        return activityId;
    }

    public void setActivityId(Integer activityId) {
        this.activityId = activityId;
    }

    public Integer getVisible() {
        return visible;
    }

    public void setVisible(Integer visible) {
        this.visible = visible;
    }

    public Integer getReplayId() {
        return replayId;
    }

    public void setReplayId(Integer replayId) {
        this.replayId = replayId;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public String getSpareA() {
        return spareA;
    }

    public void setSpareA(String spareA) {
        this.spareA = spareA;
    }

    public String getSpareB() {
        return spareB;
    }

    public void setSpareB(String spareB) {
        this.spareB = spareB;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }
}
