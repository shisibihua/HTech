.select-sf{
    float: left;
    height: 30px;
    line-height: 30px;
    width: 230px;
}
.select-sf > span{
    width: 100px;
    display: block;
    height: 30px;
    float: left;
    line-height: 30px;
}
.select-sf > span>span{
    float: left;
    display: block;
    width: 14px;
    height: 14px;
    border-radius: 100%;
    cursor: pointer;
    margin-top: 8px;
    margin-right: 10px;
    border:1px solid #dbdbdb;
}
.select-sf > span>span.color-gree{
    border:1px solid #2e8ded;
}
.select-sf > span>span i{
    display: block;
    width: 10px;
    height: 10px;
    margin-top: 2px;
    margin-left: 2px;
    background: #2e8ded;
    border-radius: 100%;
}

