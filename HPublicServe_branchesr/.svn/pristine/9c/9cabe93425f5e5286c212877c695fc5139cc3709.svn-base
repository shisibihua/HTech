.layui-nav{
    background: none;
}


.user{
    float: right;
    height: 30px;
    margin-top: 25px;
    margin-right: 10px;
    line-height: 30px;
}
.user_before{
    float: left;
    width: 30px;
    height: 30px;
    border-radius: 100%;
    overflow: hidden;
}
.user_img{
    display: block;
    float: left;
    max-height: 30px;
    max-width: 30px;
    /*background: url(../images/user-head.png) no-repeat;*/
}
.user ul li a{
    display: block;
}
.user ul,.user ul li,.user ul li > a{
    line-height: 30px;
    float: right;
    height: 30px;
    min-width: 30px;
    text-align: center;
}
.user ul.layui-nav li.layui-nav-item a.user_name{
    padding: 0 20px 0 5px;
}
.user ul .layui-nav-child,.user ul .layui-nav-child a{
    top:39px;
    height:30px;
    line-height:30px;
}
.layui-nav .layui-nav-more{
    top: 13px !important;
}
.user ul .layui-nav-bar{
    background: none;
}
.layui-nav .layui-nav-more.layui-nav-mored{
    top: 7px !important;
}
.layui-nav .layui-this:after, .layui-nav-bar, .layui-nav-tree .layui-nav-itemed:after{
    background: #03c449;
}
