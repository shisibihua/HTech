.add-org-u{
    height: 30px;
    width: 100%;
    margin-top: 15px;
}
.org-input{
    width: 225px;
    height: 28px;
    line-height: 28px;
}
.add-org-t-u{
    display: block;
    width: 95px;
    float: left;
    height: 30px;
    line-height: 30px;

}
