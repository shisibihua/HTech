@import '../theme.less';
.main{
  width: 1200px;
  margin: 0 auto;
  >div{
    float: left;
    width: 260px;
    margin:50px 20px 0 20px;
    height: 30px;
    line-height: 30px;
    >span{
      border: 1px solid #bdbdbd;
      display: block;
      float: left;
      height: 30px;
      line-height: 30px;
      padding: 0 5px;
      width:180px;
      border-radius: 5px;
      text-overflow:ellipsis;
      white-space:nowrap;
      overflow:hidden;
    }
    a{
      width: 26px;
      float: left;
    }
  }
}
