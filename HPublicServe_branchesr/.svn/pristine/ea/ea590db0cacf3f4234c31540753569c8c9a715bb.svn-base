    <link rel="stylesheet" href="${pageContext.request.contextPath}/css/zTreeStyle/zTreeStyle.css"/>
    <style>
        /* 教师管理tab */
        .user-main {
            width: 100%;
            border: none;
        }
        .main-top {
            margin: 10px;
        }
        .main-list {
            width: 100%;
        }

        .u-title {
            width: 100%;
        }

        .u-title >div {
            padding: 0 5px;
        }
        .u-duty {
            width: 90px;
        }
        .u-name,.u-number,.u-org,.u-tel,.u-subject,.u-operation {
            width: 100px;
        }
        .u-item .user-head {
            height: 100%;
        }
        .u-title .u-name,.u-item .u-name {
            padding: 0;
            width: 110px;
        }
        .u-item .u-name .user-con {
            width: 70px;
            margin: 9px 0 5px 4px;
        }
        .u-item >div {
            padding: 0 5px;
        }
        .u-name img {
            width: 50px;
            height: 50px;
            margin: 16px 0;
        }

        .u-title .u-operation, .u-item .u-operation {
            width: 80px;
        }

        .campus-content {
            padding: 6px 0 4px;
            width: 250px;
            height: 30px;
            position: relative;
        }
        .campus-content {
            position: relative;
            cursor: pointer;
        }
        .arrow {
            position: absolute;
            top: 20px;
            right: 16px;
            width: 0;
            height:0;
            border: 6px solid transparent;
            border-top: 6px solid #333;
        }
        .campus-title, .campus-title-edit {
            width: 226px;
            height: 30px;
            padding: 0 10px;
            line-height: 30px;
            border: 1px solid #dbdbdb;
            margin-top: 2px;
            margin-left: 2px;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
        }
        .campusTree {
            display: none;
            position: absolute;
            top: 42px;
            left: 2px;
            width: 236px;
            border: 1px solid #dbdbdb;
            background: #fff;
            z-index: 99;
        }
        /*#campusTreeEdit {*/
            /*display: none;*/
            /*position: absolute;*/
            /*top: 42px;*/
            /*left: 2px;*/
            /*width: 236px;*/
            /*border: 1px solid #dbdbdb;*/
            /*background: #fff;*/
            /*z-index: 99;*/
        /*}*/
        .ztree li {
            line-height: 30px;
        }
        .ztree li a {
            height: 30px;
            line-height: 30px;
        }
        .ztree li a.curSelectedNode {
            background-color: #fff;
            border: none;
            height: 100%;
        }
        a:hover {
            text-decoration: none!important;
        }
        .ztree * {
            font-size: 14px;
        }

        #user-name {
            font-size: 12px;
        }
    </style>
</head>

