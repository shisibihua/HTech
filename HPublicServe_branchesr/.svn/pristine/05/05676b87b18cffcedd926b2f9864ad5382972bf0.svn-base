.winback_t{
    border-radius:3px;
    float: left;
    width: 140px;
    height:30px;
    position: fixed;
    z-index: 1000;
    background: #ddd;
    padding: 10px;

}
.winback_t_sj{
    float: left;
    width: 100%;
    position: relative;
}
.winback_t_sj::before{
    position: absolute;
    content: "";
    display: block;
    top: -19px;
    left: 10px;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-bottom:10px solid #ddd;
}
.winback_t_sj span span,.winback_t_sj span p{
    float: left;
    height: 30px;
    line-height: 30px;

}
.winback_t_sj span p{
    width: 100px;

}
