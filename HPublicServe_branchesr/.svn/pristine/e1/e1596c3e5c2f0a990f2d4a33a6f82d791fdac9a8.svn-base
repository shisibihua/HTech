    left join device_specification d on h.dspec_id = d.dspec_id
    left join device_type t on t.dtype_id = d.dtype_id
    where 1=1
  </select>

  <select id="getMcuHostInfo" parameterType="java.lang.String" resultType="java.util.HashMap">
    select
    h.host_id,
	h.host_name,
	h.host_ipaddr,
	h.host_serialno,
	h.dspec_id,
	h.host_desc,
	h.host_username,
	h.host_password,
	h.host_mac,
	h.host_hhtcmac,
	h.host_factory,
	h.host_port,
	h.host_channel,
	h.isTour,
	h.host_systype,
	h.mcu_code
    from device_host h
    left join device_specification d on h.dspec_id = d.dspec_id
    left join device_type t on t.dtype_id = d.dtype_id
    <if test="factory!=null and factory!=''">
      where h.host_factory=#{factory}
    </if>
    <if test="factory==null or factory==''">
      where h.host_factory!='HiteTech-vhd'
    </if>
  </select>
  <select id="getConditionsHostListByPage" resultType="java.util.HashMap">
    select h.host_id,h.host_ipaddr,h.host_name,h.mcu_code,t.dtype_name,
    t.dtype_name_cn,r.name from device_host h
    left join device_specification d on h.dspec_id = d.dspec_id
    left join device_type t on t.dtype_id = d.dtype_id
    left join ad_area2device a on a.device_id = h.host_id
    left join ad_area r on r.id = a.area_id
    where 1=1
    <if test='areaId!=null and areaId!=""'>
      <choose>
        <when test='areaId =="-1"'>
          and h.host_id not in (SELECT DISTINCT device_id FROM ad_area2device)
        </when>
        <otherwise>
          and h.host_id in (SELECT DISTINCT device_id FROM ad_area2device a2d WHERE  a2d.area_id = #{areaId,jdbcType=VARCHAR})
        </otherwise>
      </choose>
    </if>
    <if test='deviceType!=null and deviceType!=""'>
      <choose>
        <when test='deviceType =="HHREC"'>
          and t.dtype_name IN ('NVR','IPC','AREC')
        </when>
        <otherwise>
          and t.dtype_name = #{deviceType,jdbcType=VARCHAR}
        </otherwise>
      </choose>
    </if>
    <if test='conditions!=null and conditions!=""'>
      and (h.host_name LIKE CONCAT('%', #{conditions}, '%') OR h.host_ipaddr LIKE CONCAT('%', #{conditions}, '%'))
    </if>
    order by h.host_id
    <if test="start>=0 and end>0">
      limit #{start},#{end}
    </if>

  </select>
  <select id="getHostInfoByPage" resultType="java.util.HashMap">
    select h.host_id,h.host_name,h.host_ipaddr,h.host_serialno,
    h.host_desc,h.host_username,h.host_password,h.dspec_id,
    d.dtype_id,t.dtype_name,h.host_port,h.host_channel,h.mcu_code,r.name,t.dtype_name_cn from device_host h
    left join device_specification d on h.dspec_id = d.dspec_id
    left join device_type t on t.dtype_id = d.dtype_id
    left join ad_area2device a on a.device_id = h.host_id
    left join ad_area r on a.area_id = r.id
    where 1=1
    <if test='deviceType!=null and deviceType!=""'>
      <choose>
        <when test='deviceType =="HHREC"'>
          and t.dtype_name IN ('NVR','IPC','AREC')
        </when>
        <otherwise>
          and t.dtype_name = #{deviceType,jdbcType=VARCHAR}
        </otherwise>
      </choose>
    </if>
    <if test="hostIdStr!=null and hostIdStr.length>0 ">
      and
      host_id in
      <foreach item="item" collection="hostIdStr" open="(" separator="," close=")">
        #{item}
      </foreach>
    </if>
    <if test="start>=0 and end>0">
      limit #{start},#{end}
    </if>

  </select>

