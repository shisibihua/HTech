@import '../theme.less';

:global(.ant-menu-inline){
  min-height: calc(100vh - 160px);
  min-height: 100%;
}
:global(.ant-menu-item-selected > a) {
  color: @main-color;
}
:global(.ant-menu-inline .ant-menu-item-selected) {
  border-right-color: @main-color;
}
.list{
  >i{
    margin-left:20px;
    font-size: 16px;
  }
}
